# cache ttl (seconds)
MARKET_CACHE_QUOTE_TTL_SEC="30"
MARKET_CACHE_HISTORY_TTL_SEC="300"
MARKET_CACHE_INDICATORS_TTL_SEC="300"
MARKET_CACHE_NEWS_TTL_SEC="600"
