# prisma datasource
DATABASE_PROVIDER="postgresql"
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/investment_dashboard?schema=public"

# switch provider to "mock" or "real" in future
MARKET_DATA_PROVIDER="mock"
ALPHA_VANTAGE_API_KEY=""
